INP_Str1 = input('Enter the first string: ')
INP_Str2 = input('Enter the second string: ')
print('You entered string1 = %s \n string2 = %s'%(INP_Str1 ,INP_Str2))
if(len(INP_Str1)==len(INP_Str2)):
    if(INP_Str2[::]==INP_Str1[-1::-1]):
        print('Right shift.')
    else:print('No right shift.')
else:print('Please enter the string of equal length.')
